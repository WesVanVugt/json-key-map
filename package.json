{
    "name": "json-key-map",
    "version": "0.0.1",
    "description": "A module for mapping between JSON keys and values",
    "keywords": [
        "json",
        "key",
        "map",
        "composite",
        "composite-key",
        "typescript"
    ],
    "author": "Wes van Vugt <wes.vanvugt@gmail.com>",
    "repository": "WesVanVugt/json-key-map",
    "scripts": {
        "prepare": "npm run build",
        "build": "npm run clean && tsc",
        "clean": "rm -rf index.js index.d.ts *.tgz",
        "test": "npm run lint && nyc mocha --require ts-node/register ts/test/*.test.ts --color",
        "test-package": "npm pack && cd test-package && npm i && npm test",
        "release": "np",
        "lint": "tslint ts/**/*.ts"
    },
    "main": "index.js",
    "typings": "index.d.ts",
    "engines": {
        "node": ">=4"
    },
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "dependencies": {},
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/json-stable-stringify": "^1.0.32",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.21",
        "chai": "^4.2.0",
        "json-stable-stringify": "^1.0.1",
        "mocha": "^5.2.0",
        "np": "^4.0.2",
        "nyc": "^13.1.0",
        "ts-node": "^8.0.2",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1"
    },
    "license": "MIT",
    "nyc": {
        "extension": [
            ".ts"
        ]
    }
}
